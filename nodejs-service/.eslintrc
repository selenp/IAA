{
  "extends": "eslint-config-airbnb",
  "rules": {
    "camelcase": "off",
    "class-methods-use-this": [
      0
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "no-continue": [
      0
    ],
    "spaced-comment": [
      0,
      "always"
    ]
  }
}